{
  "name": "saleor-app-payment-razorpay",
  "version": "1.0.1",
  "private": true,
  "license": "(BSD-3-Clause AND CC-BY-4.0)",
  "type": "module",
  "scripts": {
    "build": "next build",
    "check-types": "tsc --noEmit",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "fetch-schema": "curl https://raw.githubusercontent.com/saleor/saleor/${npm_package_config_saleor_schemaVersion}/saleor/graphql/schema.graphql > graphql/schema.graphql",
    "generate": "pnpm run /generate:.*/",
    "generate:app-graphql-types": "graphql-codegen",
    "generate:app-webhooks-types": "tsx ./scripts/generate-app-webhooks-types.ts",
    "lint": "next lint",
    "start": "next start",
    "test": "vitest"
  },
  "config": {
    "saleor": {
      "schemaVersion": "3.22"
    }
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.989.0",
    "@aws-sdk/lib-dynamodb": "^3.989.0",
    "@saleor/app-sdk": "1.6.0",
    "@saleor/macaw-ui": "1.4.1",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@urql/exchange-auth": "^1.0.0",
    "dynamodb-toolbox": "^2.8.0",
    "next": "15.0.0",
    "razorpay": "^2.9.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "urql": "^4.0.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/schema-ast": "^3.0.1",
    "@graphql-codegen/typed-document-node": "4.0.1",
    "@graphql-codegen/typescript": "3.0.4",
    "@graphql-codegen/typescript-operations": "3.0.4",
    "@graphql-codegen/typescript-urql": "^3.7.3",
    "@graphql-codegen/urql-introspection": "^3.0.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.31.0",
    "eslint-config-next": "14.0.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "jsdom": "^20.0.3",
    "json-schema-to-typescript": "^15.0.4",
    "tsx": "4.20.3",
    "typescript": "5.0.4",
    "vite": "5.2.10",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "1.5.2"
  },
  "engines": {
    "node": ">=22.0.0 <23.0.0",
    "npm": ">=10.0.0 <11.0.0",
    "pnpm": ">=10.0.0 <11.0.0"
  }
}
